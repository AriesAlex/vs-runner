{
  "path": "$HOMEDIR/Documents/Projects",
  "projectsPerPage": 5,
  "templates": {
    "_OPEN_PROJECT": "code $PROJECT_PATH",
    "NodeJs": [
      "mkdir $PROJECT_PATH",
      { "cmd": "echo //TODO>main.js", "cwd": "project" },
      { "cmd": "npm init -y", "cwd": "project" }
    ],
    "Vue": "vue create $NAME",
    "Vue+Electron": [
      "vue create $NAME",
      { "cmd": "vue add electron-builder", "cwd": "project" }
    ],
    "Nuxt": "npx nuxi init $NAME",
    "Electron": "npx create-electron-app $NAME",
    "Svelte": [
      "npm create svelte@latest $NAME",
      { "cmd": "npm i", "cwd": "project" }
    ],
    "React": "npx create-react-app $NAME",
    "React Native": "npx create-react-native-app $NAME",
    "React Native Expo": "expo init $NAME",
    "AdoniaJS": "npm init adonis-ts-app@latest $NAME",
    "PHP Laravel": "laravel new $NAME",
    "Flutter": "flutter create $NAME",
    "Tauri": "npm create tauri-app $NAME"
  }
}